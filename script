local function IsKeyValid(inputKey)
	-- Replace this with your actual key validation logic
	local validKeys = {
		"subtorbx",
		"rbxsterontop",
		"sub2"
		-- Add more valid keys as needed
	}

	for _, key in ipairs(validKeys) do
		if inputKey == key then
			return true
		end
	end

	return false
end

-- Function to handle the submission of the key
local function HandleKeySubmission()
	local inputKey = Key.Text
	if IsKeyValid(inputKey) then
		-- Key is valid, perform action here
		print("Key is valid! You can now unlock something.")

		-- Close the current frame
		Frame:Destroy()

		-- Create and configure the new frame
		AfterLoggin.Visible = true
		-- Configure properties and layout for the new frame as needed

	else
		-- Key is invalid, display an error message or perform another action
		print("Invalid key. Please try again.")
	end
end

-- Connect the submission button to the handling function
Submit.MouseButton1Click:Connect(HandleKeySubmission)

-- Close the GUI when the "X" button is clicked
TextButton.MouseButton1Click:Connect(function()
	Frame:Destroy()
end)
